{
  "name": "ts-rakentaja",
  "version": "0.1.0",
  "description": "Utility pack to build TypeScript faster and more efficient",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "clean": "git clean -fx . -e .vite -e .idea -e node_modules",
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest --run",
    "test:coverage": "vitest --run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ausginer/ts-rakentaja.git"
  },
  "keywords": [
    "typescript",
    "constructor",
    "builder",
    "ast"
  ],
  "author": "Vlad Rindevich <vladrin@vaadin.com> (https://github.com/Lodin)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ausginer/ts-rakentaja/issues"
  },
  "homepage": "https://github.com/ausginer/ts-rakentaja#readme",
  "peerDependencies": {
    "typescript": "5"
  },
  "devDependencies": {
    "@types/chai": "5.0.1",
    "@types/chai-like": "1.1.3",
    "@types/node": "22.13.1",
    "@vitest/coverage-v8": "3.0.5",
    "chai": "5.1.2",
    "chai-like": "^1.1.3",
    "eslint": "8.57.0",
    "eslint-config-vaadin": "1.0.0-alpha.28",
    "eslint-plugin-tsdoc": "0.4.0",
    "prettier": "3.5.0",
    "tsx": "4.19.2",
    "vitest": "^3.0.5"
  }
}
